{
  "name": "napp/dbalcore",
  "description": "DBAL core for projects",
  "keywords": [
    "napp",
    "dbal",
    "database",
    "criteria"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mads MÃ¸ller",
      "email": "mm@napp.dk"
    }
  ],
  "autoload": {
    "psr-4": {
      "Napp\\Core\\Dbal\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Napp\\Core\\Dbal\\Tests\\": "tests/",
      "stubs\\": "tests/_support/Stubs",
      "Helper\\": "tests/_support/Helper"
    }
  },
  "require": {
    "php" : ">=7.0",
    "illuminate/database": "~5.5.0|~5.6.0",
    "illuminate/support": "~5.5.0|~5.6.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.7",
    "mockery/mockery": "^0.9.5",
    "phpunit/phpunit" : "^6.1",
    "phpunit/phpcov": "^4.0",
    "codeception/codeception": "2.*",
    "codeception/c3": "2.*",
    "squizlabs/php_codesniffer": "^3.1"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Napp\\Core\\Dbal\\Criteria\\CriterionServiceProvider"
      ],
      "aliases": {}
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
