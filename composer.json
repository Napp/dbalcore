{
  "name": "napp/dbalcore",
  "description": "DBAL core for projects",
  "keywords": [
    "napp",
    "dbal",
    "database",
    "criteria"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mads MÃ¸ller",
      "email": "mm@napp.dk"
    }
  ],
  "autoload": {
    "psr-4": {
      "Napp\\Core\\Dbal\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Napp\\Core\\Dbal\\Tests\\": "tests/"
    }
  },
  "require": {
    "php" : "^7.1|^8.0",
    "illuminate/database": "^5.8|^6.0|^7.0|^8.0",
    "illuminate/support": "^5.8|^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpunit/phpcov": "^6.0|^7.0|^8.0",
    "squizlabs/php_codesniffer": "^3.1",
    "orchestra/testbench": "^5.0|^6.3"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Napp\\Core\\Dbal\\Criteria\\CriteriaServiceProvider",
        "Napp\\Core\\Dbal\\Builder\\BuilderServiceProvider"
      ],
      "aliases": {}
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
